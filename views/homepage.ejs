<script>
    socket = io.connect('http://localhost:1337/?token=' + localStorage.getItem('accessToken'));
    
    socket.on('connect', function () {
        console.log('connected');
        console.log(socket);                         

        socket.on('transaction', function(message) {
            console.log("message");
            console.log(message);
        });

        socket.on('disconnect', function() {
            console.log('disconnected');
        });
    });
</script>
</body>
</html>